def factorial(num):
    if num < 0:
        return "Factorial of negative numbers is not defined"
    elif num == 0:
        return 1
    else:
        fact = 1
        for i in range(1, num + 1):
            fact = fact * i
        return fact


num = int(input("Enter a number: "))
result = factorial(num)
print("The factorial of", num, "is:", result)
